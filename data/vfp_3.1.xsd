-->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.hsi.cz/vfp" targetNamespace="http://www.hsi.cz/vfp">
	<xs:simpleType name="coordinateAttrType">
		<xs:annotation>
			<xs:documentation>Složka souřadnice</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:fractionDigits value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="coordinateType">
		<xs:annotation>
			<xs:documentation>Souřadnice</xs:documentation>
		</xs:annotation>
		<xs:attribute name="x" type="coordinateAttrType" use="required"/>
		<xs:attribute name="y" type="coordinateAttrType" use="required"/>
		<xs:attribute name="z" type="coordinateAttrType" use="optional"/>
	</xs:complexType>
	<xs:complexType name="polygonType" abstract="true">
		<xs:annotation>
			<xs:documentation>Bázový typ polygonu</xs:documentation>
		</xs:annotation>
	</xs:complexType>
	<xs:complexType name="linpol">
		<xs:annotation>
			<xs:documentation>Lineární polygon</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="polygonType">
				<xs:sequence>
					<xs:element name="segment" type="segmentType" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="linearType">
		<xs:annotation>
			<xs:documentation>Linie</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="segment" type="segmentType" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="circle">
		<xs:annotation>
			<xs:documentation>Kruh</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="polygonType">
				<xs:sequence>
					<xs:element name="c" type="coordinateType"/>
				</xs:sequence>
				<xs:attribute name="r" type="radiusAttrType" use="required"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="solidType">
		<xs:annotation>
			<xs:documentation>Vyplněný polygon</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="polygon" type="polygonType"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="holeCollectionType">
		<xs:annotation>
			<xs:documentation>Díra</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="polygon" type="polygonType" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="regionType">
		<xs:annotation>
			<xs:documentation>Region. Skládá se z jednoho obvodového polygonu a může obsahoat díry</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="solid" type="solidType" maxOccurs="1"/>
			<xs:element name="holes" type="holeCollectionType" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="segmentType" abstract="true">
		<xs:annotation>
			<xs:documentation>Bázový typ segmentu</xs:documentation>
		</xs:annotation>
	</xs:complexType>
	<xs:complexType name="se">
		<xs:annotation>
			<xs:documentation>Liniový segment</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="segmentType">
				<xs:sequence>
					<xs:element name="c" type="coordinateType" minOccurs="2" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ar">
		<xs:annotation>
			<xs:documentation>Oblouk</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="segmentType">
				<xs:sequence>
					<xs:element name="c" type="coordinateType" minOccurs="3" maxOccurs="3"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:simpleType name="radiusAttrType">
		<xs:annotation>
			<xs:documentation>Průměr</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal"/>
	</xs:simpleType>
	<xs:simpleType name="rotationAttrType">
		<xs:annotation>
			<xs:documentation>Úhel v radiánech</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal"/>
	</xs:simpleType>
	<xs:simpleType name="justificationAttrType">
		<xs:annotation>
			<xs:documentation>Zarovnání textu</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:enumeration value="11">
				<xs:annotation>
					<xs:documentation>spodní levý okraj text boxu</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="12">
				<xs:annotation>
					<xs:documentation>spodní levý okraj text boxu</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="13">
				<xs:annotation>
					<xs:documentation>spodní okraj text boxu, střed</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="14">
				<xs:annotation>
					<xs:documentation>spodní pravý okraj text boxu</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="15">
				<xs:annotation>
					<xs:documentation>spodní pravý okraj text boxu</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="21">
				<xs:annotation>
					<xs:documentation>spodní levý okraj textu</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="22">
				<xs:annotation>
					<xs:documentation>spodní levý okraj textu</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="23">
				<xs:annotation>
					<xs:documentation>spodní střed textu</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="24">
				<xs:annotation>
					<xs:documentation>spodní pravý okraj textu</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="25">
				<xs:annotation>
					<xs:documentation>spodní pravý okraj textu</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="31">
				<xs:annotation>
					<xs:documentation>levý střední okraj textu</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="32">
				<xs:annotation>
					<xs:documentation>levý střední okraj textu</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="33">
				<xs:annotation>
					<xs:documentation>střed textu</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="34">
				<xs:annotation>
					<xs:documentation>pravý střední okraj textu</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="35">
				<xs:annotation>
					<xs:documentation>pravý střední okraj textu</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="41">
				<xs:annotation>
					<xs:documentation>levý horní okraj textu</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="42">
				<xs:annotation>
					<xs:documentation>levý horní okraj textu</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="51">
				<xs:annotation>
					<xs:documentation>levý horní okraj textu</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="52">
				<xs:annotation>
					<xs:documentation>levý horní okraj textu</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="53">
				<xs:annotation>
					<xs:documentation>horní střed</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="43">
				<xs:annotation>
					<xs:documentation>horní střed</xs:documentation>
				</xs:annotation>
			</xs:enumeration>			
			<xs:enumeration value="44">
				<xs:annotation>
					<xs:documentation>horní pravý okraj textu</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="45">
				<xs:annotation>
					<xs:documentation>horní pravý okraj textu</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="54">
				<xs:annotation>
					<xs:documentation>horní pravý okraj textu</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="55">
				<xs:annotation>
					<xs:documentation>horní pravý okraj textu</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="heightAttrType">
		<xs:annotation>
			<xs:documentation>Výška</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal"/>
	</xs:simpleType>
	<xs:simpleType name="widthAttrType">
		<xs:annotation>
			<xs:documentation>Šířka</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal"/>
	</xs:simpleType>
	<xs:simpleType name="valueAttrType">
		<xs:annotation>
			<xs:documentation>Základní číselná hodnota</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal"/>
	</xs:simpleType>
	<xs:complexType name="textType">
		<xs:annotation>
			<xs:documentation>Text v grafice</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="c" type="coordinateType">
				<xs:annotation>
					<xs:documentation>Souřadnice</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="hod" type="notEmptyStringType" use="required">
			<xs:annotation>
				<xs:documentation>Text</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="o" type="rotationAttrType" use="optional">
			<xs:annotation>
				<xs:documentation>Natočení textu</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="j" type="justificationAttrType" use="optional">
			<xs:annotation>
				<xs:documentation>Zarovnání</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="vys" type="heightAttrType" use="optional">
			<xs:annotation>
				<xs:documentation>Výška</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="sir" type="widthAttrType" use="optional">
			<xs:annotation>
				<xs:documentation>Šířka</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="cellType">
		<xs:annotation>
			<xs:documentation>Značka (buňka)</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="c" type="coordinateType">
				<xs:annotation>
					<xs:documentation>Souřadnice</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="o" type="rotationAttrType" use="optional">
			<xs:annotation>
				<xs:documentation>Natočení</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="naz" type="notEmptyStringType" use="optional">
			<xs:annotation>
				<xs:documentation>Název</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:simpleType name="cadasterCodeType">
		<xs:annotation>
			<xs:documentation>Kód katastrálního území</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="600000"/>
			<xs:maxInclusive value="999999"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="lvNumberAttrType">
		<xs:annotation>
			<xs:documentation>Číslo listu vlastnictví</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="-99999"/>
			<xs:maxInclusive value="99999"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="numeratorType">
		<xs:annotation>
			<xs:documentation>Čitatel vlastnického podílu podílu</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="denominatorType">
		<xs:annotation>
			<xs:documentation>Jmenovatel vlastnického podílu</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="areaAttrType">
		<xs:annotation>
			<xs:documentation>Plocha v (m2)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:minInclusive value="0"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="priceAttrType">
		<xs:annotation>
			<xs:documentation>Cena</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:minInclusive value="0"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="distanceType">
		<xs:annotation>
			<xs:documentation>Vzdálenost</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:minInclusive value="0"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="notEmptyStringType">
		<xs:annotation>
			<xs:documentation>Neprázdný textový řetězec</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="opsubIdType">
		<xs:annotation>
			<xs:documentation>ID oprávněného subjektu</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="bpejType">
		<xs:annotation>
			<xs:documentation>Typ oceňovacího dílu</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:enumeration value="1">
				<xs:annotation>
					<xs:documentation>areál BPEJ</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="2">
				<xs:annotation>
					<xs:documentation>areál SLT</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="3">
				<xs:annotation>
					<xs:documentation>uživatelský kód</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="bpejCodeAttrType">
		<xs:annotation>
			<xs:documentation>Kód BPEJ</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="5"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="birthNumberType">
		<xs:annotation>
			<xs:documentation>Rodné číslo</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="6"/>
			<xs:maxLength value="10"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="streetNumberType">
		<xs:annotation>
			<xs:documentation>Číslo domovní</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="0"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="orientationNumberType">
		<xs:annotation>
			<xs:documentation>Číslo orientační</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="4"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="postCodeType">
		<xs:annotation>
			<xs:documentation>PSČ</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="5"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="emailType">
		<xs:annotation>
			<xs:documentation>E-mail</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value=".*@.*"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="icoType">
		<xs:annotation>
			<xs:documentation>IČO</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="0"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="koeficientType">
		<xs:annotation>
			<xs:documentation>Obecný koeficient</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:minInclusive value="0"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="participantType">
		<xs:annotation>
			<xs:documentation>Typ účastníka</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:enumeration value="1">
				<xs:annotation>
					<xs:documentation>Vlastník</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="2">
				<xs:annotation>
					<xs:documentation>Okruh dědiců</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="3">
				<xs:annotation>
					<xs:documentation>Opatrovník</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="4">
				<xs:annotation>
					<xs:documentation>Zástupce vlastníka - plná moc</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="5">
				<xs:annotation>
					<xs:documentation>Zástupce - právnické osoby</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="6">
				<xs:annotation>
					<xs:documentation>Účastník z jiného právního vztahu</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="7">
				<xs:annotation>
					<xs:documentation>Ostatní</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="basicIdAttrType">
		<xs:annotation>
			<xs:documentation>Obecné číselné ID</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer"/>
	</xs:simpleType>
	<xs:simpleType name="kindOfUseAttrType">
		<xs:annotation>
			<xs:documentation>Způsob využití dle číselníku ISKN</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal"/>
	</xs:simpleType>
	<xs:simpleType name="kindOfAreaAttrType">
		<xs:annotation>
			<xs:documentation>Druh pozemku dle číselníku ISKN</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal"/>
	</xs:simpleType>
	<xs:simpleType name="basicStringIdAttrType">
		<xs:annotation>
			<xs:documentation>Základní id jako text</xs:documentation>
		</xs:annotation>
		<xs:restriction base="notEmptyStringType"/>
	</xs:simpleType>
	<xs:simpleType name="participantIdAttrType">
		<xs:annotation>
			<xs:documentation>ID účastníka</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer"/>
	</xs:simpleType>
	<xs:simpleType name="parcelIdAttrType">
		<xs:annotation>
			<xs:documentation>ID parcely</xs:documentation>
		</xs:annotation>
		<xs:restriction base="basicStringIdAttrType"/>
	</xs:simpleType>
	<xs:simpleType name="noteAttrType">
		<xs:annotation>
			<xs:documentation>Poznámka</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="0"/>
			<xs:maxLength value="255"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="outsideParcelType">
		<xs:annotation>
			<xs:documentation>Parcely mimo obvod</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="pa" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="gpar" type="gparType">
							<xs:annotation>
								<xs:documentation>Grafika</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="parid" type="parcelIdAttrType" use="required">
						<xs:annotation>
							<xs:documentation>ID parcely</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="newNumberType">
		<xs:annotation>
			<xs:documentation>Parcela s novým číslem</xs:documentation>
		</xs:annotation>
		<xs:attribute name="vym" type="areaAttrType" use="required"/>
		<xs:attribute name="kk" type="cadasterCodeType" use="required"/>
		<xs:attribute name="kcp" type="parcelNumberAttrType" use="required"/>
		<xs:attribute name="pcp" type="poddeleniNumberAttrType" use="optional"/>
		<xs:attribute name="dcp" type="druhCislovaniNumberAttrType" use="required"/>
	</xs:complexType>
	<xs:complexType name="newNumberCollectionType">
		<xs:annotation>
			<xs:documentation>Seznam parcel s novým číslem</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="nc" type="newNumberType" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="pneresType">
		<xs:annotation>
			<xs:documentation>Neřešené parcely</xs:documentation>
		</xs:annotation>
