				<xs:annotation>
					<xs:documentation>Mimo církev</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="1">
				<xs:annotation>
					<xs:documentation>Určené pro církev</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
