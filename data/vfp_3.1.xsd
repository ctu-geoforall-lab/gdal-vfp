					<xs:documentation>verze pro 1. rozhodnutí</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="4">
				<xs:annotation>
					<xs:documentation>verze pro 2. rozhodnutí</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
